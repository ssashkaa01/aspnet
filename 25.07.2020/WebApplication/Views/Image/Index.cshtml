
@{

    ViewBag.Title = "Images";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Images</h2>


<div class="container">
    <div class="row">
        @foreach (WebApplication.Entities.Image item in Model.Images)
        {
            <div class="card col-md-4">
                <img class="card-img-top" src="@item.Url">
            </div>
        }
    </div>
</div>
        